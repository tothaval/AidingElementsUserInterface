<UserControl x:Class="AidingElementsUserInterface.Core.AEUI_UserControls.CoreMatrixLine"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AidingElementsUserInterface.Core.AEUI_UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    
    <StackPanel Orientation="Horizontal">


                <Border
            CornerRadius="15">
                    <Button x:Name="BTN_minus"
                    Content="-"
                    Click="BTN_minus_Click"
                    Background="Transparent"
                    Foreground="Aquamarine"
                    Width="20"/>
                </Border>
        <Border
            CornerRadius="15">
            <Button x:Name="BTN_plus"
                    Content="+"
                    Click="BTN_plus_Click"
                    Background="Transparent"
                    Foreground="Aquamarine"
                    Width="20"
                    />
        </Border>
        <StackPanel x:Name="SP" Orientation="Horizontal"/>
        

    </StackPanel>
</UserControl>
