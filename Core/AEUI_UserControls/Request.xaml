    <UserControl x:Class="AidingElementsUserInterface.Core.AEUI_UserControls.Request"
             x:Name="__Request"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:core="clr-namespace:AidingElementsUserInterface.Core.AEUI_SystemControls"
             xmlns:local="clr-namespace:AidingElementsUserInterface.Core.AEUI_UserControls"             
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Border>
        <StackPanel Orientation="Vertical">
            <core:LogStat x:Name="LS_Protocol"/>

            <local:Command x:Name="C_Console" HorizontalAlignment="Stretch"/>

        </StackPanel>            
    </Border>
</UserControl>
